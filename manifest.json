{
  "manifest_version": 2,
  "name": "InfinityCam",
  "short_name": "InfinityCam",
  "author": "Max Helmetag",
  "version": "1.0",
  "version_name": "1.0",
  "description": "Watch Surfline Cams for (basically) Infinity with No Ads",
  "content_scripts": [
    {
      "matches": [
        "http://www.surfline.com/surf-report/*"
      ],
      "js": [
        "build/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "build/script.js"
  ]
}
